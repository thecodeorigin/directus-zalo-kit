{
  "name": "@thecodeorigin/directus-zalo-kit",
  "type": "module",
  "version": "1.0.0",
  "description": "Please enter a description for your extension",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-bundle"
  ],
  "icon": "extension",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "display",
        "name": "directus-display-boilerplate",
        "source": "src/display/index.ts"
      },
      {
        "type": "endpoint",
        "name": "zalo",
        "source": "src/endpoint/index.ts"
      },
      {
        "type": "interface",
        "name": "directus-interface-boilerplate",
        "source": "src/interface/index.ts"
      },
      {
        "type": "layout",
        "name": "directus-layout-boilerplate",
        "source": "src/layout/index.ts"
      },
      {
        "type": "operation",
        "name": "directus-operation-boilerplate",
        "source": {
          "app": "src/operation/app.ts",
          "api": "src/operation/api.ts"
        }
      },
      {
        "type": "panel",
        "name": "directus-panel-boilerplate",
        "source": "src/panel/index.ts"
      },
      {
        "type": "theme",
        "name": "directus-theme-boilerplate",
        "source": "src/theme/index.ts"
      },
      {
        "type": "module",
        "name": "zalo-chat",
        "source": "src/zalo-chat/index.ts"
      },
      {
        "type": "module",
        "name": "zalo-listener",
        "source": "src/zalo-listener/index.ts"
      }
    ],
    "host": "^10.10.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "validate": "directus-extension validate",
    "lint": "eslint ."
  },
  "dependencies": {
    "ioredis": "catalog:",
    "sass": "catalog:"
  },
  "devDependencies": {
    "@directus/sdk": "catalog:",
    "autoprefixer": "catalog:",
    "knex": "catalog:",
    "postcss": "catalog:",
    "tailwindcss": "catalog:"
  }
}
